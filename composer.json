{
  "name": "chrisharrison/vo-generator",
  "description": "PHP value object generator",
  "authors": [
    {
      "name": "Chris Harrison",
      "email": "c.harrison1988@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "funeralzone/valueobjects": "^0.4.5",
    "beberlei/assert": "^3.2",
    "twig/twig": "^3.0",
    "symfony/yaml": "^5.0",
    "php-di/php-di": "^6.0",
    "composer/composer": "^1.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "squizlabs/php_codesniffer": "^3.5",
    "phpspec/prophecy": "^1.10",
    "slevomat/coding-standard": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "ChrisHarrison\\VoGenerator\\": "src/"
    },
    "files": ["autoload.php"]
  },
  "bin": [
    "vostubs"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true
  }
}
